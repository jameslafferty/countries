{
  "name": "countries",
  "description": "A utility for working with countries.",
  "version": "1.0.2",
  "homepage": "https://github.com/jameslafferty/countries",
  "author": {
    "name": "jameslafferty",
    "email": "james@nearlysensical.com",
    "url": "https://github.com/jameslafferty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jameslafferty/countries.git"
  },
  "bugs": {
    "url": "https://github.com/jameslafferty/countries/issues"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build:json": "cross-env find ./raw-data/csv -type f -exec ts-node --dir ./scripts processCSV.ts {} +",
    "build:cjs": "tsc -p ./tsconfig/tsconfig.cjs.json",
    "build:declaration": "tsc -p ./tsconfig/tsconfig.declaration.json --emitDeclarationOnly",
    "build:esm": "tsc -p ./tsconfig/tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm && yarn build:declaration",
    "prebuild": "rimraf dist && yarn test:cicd",
    "prepare": "yarn build:json",
    "prepublishOnly": "yarn build:publish",
    "sb": "sb",
    "test:cicd": "cross-env CI=true react-scripts test --env=jsdom-fourteen --coverage",
    "test": "react-scripts test --env=jsdom-fourteen",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/core": "3.* || 4.* || 5.*",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-loader": "^7.0.0 || ^8.0.0",
    "cross-env": "^7.0.0",
    "csvtojson": "^2.0.10",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "husky": "^4.2.3",
    "immer": "^5.3.6",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^3.0.0 || ^4.0.0",
    "yarn": "^1.22.0"
  },
  "keywords": []
}
