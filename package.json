{
  "name": "countries",
  "description": "A utility for working with countries.",
  "version": "1.0.2",
  "homepage": "https://github.com/jameslafferty/countries",
  "author": {
    "name": "jameslafferty",
    "email": "james@nearlysensical.com",
    "url": "https://github.com/jameslafferty"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jameslafferty/countries.git"
  },
  "bugs": {
    "url": "https://github.com/jameslafferty/countries/issues"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "build:json": "cross-env find ./raw-data/csv -type f -exec ts-node --dir ./scripts processCSV.ts {} +",
    "build:cjs": "tsc -p ./tsconfig/tsconfig.cjs.json",
    "build:declaration": "tsc -p ./tsconfig/tsconfig.declaration.json --emitDeclarationOnly",
    "build:esm": "tsc -p ./tsconfig/tsconfig.esm.json",
    "build": "yarn build:cjs && yarn build:esm && yarn build:declaration",
    "prebuild": "rimraf dist && yarn test:cicd",
    "prepare": "yarn build:json",
    "prepublishOnly": "yarn build:publish",
    "sb": "sb",
    "test:cicd": "cross-env CI=true react-scripts test --env=jsdom-fourteen --coverage",
    "test": "react-scripts test --env=jsdom-fourteen",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/core": "3.* || 4.* || 5.*",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@storybook/react": "^5.3.19",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.0",
    "csvtojson": "^2.0.10",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "^4.2.3",
    "immer": "^7.0.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "yarn": "^1.22.4"
  },
  "keywords": []
}
